package week1.day2;

public class Fibonacci_series {

	public static void main(String[] args) {
		// logic-->by adding the previous two numbers to get the next term)
		// 0 1 1 2 3 5 8 13 excepted output
		int input_range=8;
		int first_num=0;
		int second_num=1;
		for(int i=0;i<8;i++)
		{
			System.out.print(first_num+" ");
			int result= first_num+second_num;// 3rd number
			
			first_num=second_num; // making second number as first number
			second_num=result; // making third number as second number
	
		}
		

	}

}
